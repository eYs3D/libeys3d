<launch>
  <include file="$(find eys3d_robot)/launch/lidar/$(env EYS3DLIDAR).launch" />

  <arg name="map_file" default="$(find eys3d_robot)/maps/house.yaml"/>
  <node pkg="map_server" name="map_server"  type="map_server" args="$(arg map_file)" />

  <include file="$(find eys3d_robot)/launch/amcl.launch" />
  <include file="$(find eys3d_robot)/param/navigation/move_base.xml" />
  <node name="robot_pose_publisher" pkg="robot_pose_publisher" type="robot_pose_publisher" />
  <node name="multi_mark" pkg="clbrobot" type="show_mark.py" output="screen" />
</launch>
