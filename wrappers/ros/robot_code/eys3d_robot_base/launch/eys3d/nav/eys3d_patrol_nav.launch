    <param name="rest_time"     value="5" />
    <param name="keep_patrol"   value="false" />
    <param name="random_patrol" value="false" />
    <param name="patrol_type"   value="0" />
    <param name="patrol_loop"   value="5" />
    <param name="patrol_time"   value="10" />
  </node>
-->
</launch>
