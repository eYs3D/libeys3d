<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/local_costmap_params.yaml" command="load" />
    	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/global_costmap_params.yaml" command="load" />
	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/teb_local_planner_params.yaml" command="load" />
    	<rosparam file="$(find eys3d_robot)/launch/eys3d/auto_slam/config/move_base_params.yaml" command="load" />
  </node>
</launch>
